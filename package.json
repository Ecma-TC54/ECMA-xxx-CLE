{
  "private": true,
  "name": "common-lifecycle-enumeration",
  "version": "1.0.0-SNAPSHOT",
  "description": "The Common Lifecycle Enumeration specification",
  "scripts": {
    "ipr-check": "node scripts/check-form Ecma-TC54/ECMA-xxx-CLE HEAD --all",
    "build-head": "npm run build-only -- --lint-spec --strict",
    "prebuild-only": "npm run clean && mkdir out && cp -R img out",
    "build-only": "ecmarkup --verbose spec.emu --multipage out",
    "build": "npm run build-head",
    "build-for-pdf": "npm run prebuild-only && ecmarkup --verbose spec.emu out/index.html --assets external --assets-dir out --printable --lint-spec --strict",
    "pdf": "npm run build-for-pdf && prince-books --script ./node_modules/ecmarkup/js/print.js out/index.html -o out/ECMA-CLE.pdf",
    "prebuild-snapshot": "npm run clean",
    "build-snapshot": "npm run build-head && node scripts/insert_snapshot_warning.js",
    "clean": "rm -rf out",
    "format": "emu-format --write spec.emu",
    "test": "exit 0",
    "watch": "npm run build-only -- --lint-spec --watch",
    "check-commit": "node scripts/check-commit"
  },
  "repository": "Ecma-TC54/ECMA-xxx-CLE",
  "author": "ECMA TC54",
  "homepage": "https://tc54.org/cle/",
  "devDependencies": {
    "ecmarkup": "^22.0.0",
    "pagedjs": "^0.4.3",
    "pagedjs-cli": "^0.4.3"
  }
}
